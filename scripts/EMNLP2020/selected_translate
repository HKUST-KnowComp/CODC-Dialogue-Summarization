     -batch_size 20 \
     -beam_size 4 \
     -model dep_models/all_caps_baseline/trans_copy_step_40000.pt \
     -src  dataset/Dial2Desc/dialog.test.txt \
     -output selected_item/base_trans_all \
     -min_length 5 \
     -max_length 15 \
     -verbose

